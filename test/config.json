{
    "env": {
        "NODE_ENV": "development"
    },
    "debug": {
        "active": 1
    },
    "logging": {
        "output": {
            "stdout": { "type": "console" }
        },
        "loggers": {
            "*": { "stdout": "debug" }
        },
        "http": { "logMode": "response" }
    },
    "db": {
        "hosts": "localhost",
        "database": "node-sap-app-server-test",
        "options": {
            "db": {
                "safe": { "w": 1}
            },
            "server": {
                "poolSize": 1,
                "socketOptions": {
                    "keepAlive": 1
                }
            }
        }
    },
    "deployment": {
        "strategy": "distribute"
    },
    "http": {
        "port": 9089
    },
    "services": {},
    "web": {}
}
